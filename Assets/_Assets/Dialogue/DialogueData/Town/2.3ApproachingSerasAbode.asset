  m_Script: {fileID: 11500000, guid: 6c77e930972665f4f9a9210fba3c3022, type: 3}
  m_Name: 2.3ApproachingSerasAbode
  m_EditorClassIdentifier: 
  dialogueLines:
  - speaker: {fileID: 11400000, guid: 85cd204bd9b350548bb972a14f222473, type: 2}
    text: Here we are.
  - speaker: {fileID: 11400000, guid: 85cd204bd9b350548bb972a14f222473, type: 2}
    text: "The abode which houses the incarnate of pure evil, the epicenter of everything
      wrong with this town \u2014 Sera."
  - speaker: {fileID: 11400000, guid: e7e49e1734a8be947b3d8e2d7eee3b3e, type: 2}
    text: What's wrong with them? Are they like a witch or a monster?
  - speaker: {fileID: 11400000, guid: 85cd204bd9b350548bb972a14f222473, type: 2}
    text: Ah, no, most of us in town are monsters; she's something FAR worse.
  - speaker: {fileID: 11400000, guid: e7e49e1734a8be947b3d8e2d7eee3b3e, type: 2}
    text: Wh-what is she then?
  - speaker: {fileID: 11400000, guid: 85cd204bd9b350548bb972a14f222473, type: 2}
    text: ...
  - speaker: {fileID: 11400000, guid: 85cd204bd9b350548bb972a14f222473, type: 2}
    text: She's a nerd.
  - speaker: {fileID: 11400000, guid: e7e49e1734a8be947b3d8e2d7eee3b3e, type: 2}
    text: Is ... is that it?
  - speaker: {fileID: 11400000, guid: 85cd204bd9b350548bb972a14f222473, type: 2}
    text: What more do you need?! She's always in there. READING and STUDYING, probably
      plotting, probably a racist too.
  - speaker: {fileID: 11400000, guid: 85cd204bd9b350548bb972a14f222473, type: 2}
    text: And not the right kind, like me. You can listen to my research on it on
      my podcast.
  nextDialogueObject: {fileID: 11400000, guid: 09fe5e4795777bf4fa23d320495d7dbe, type: 2}
  responses: []
